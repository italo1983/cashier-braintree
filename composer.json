{
    "abandoned": "braintree/braintree_php",
    "name": "italo1983/cashier-braintree",
    "description": "Laravel Cashier provides an expressive, fluent interface to Braintree's subscription billing services.",
    "keywords": ["laravel", "braintree", "billing", "cashier"],
    "license": "MIT",
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylorotwell@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0|^8.1",
        "braintree/braintree_php": "^6.5",
        "dompdf/dompdf": "^1.1.0",
        "illuminate/contracts": "^7.0|^8.0|^9.0",
        "illuminate/database": "^7.0|^8.0|^9.0",
        "illuminate/http": "^7.0|^8.0|^9.0",
        "illuminate/routing": "^7.0|^8.0|^9.0",
        "illuminate/support": "^7.0|^8.0|^9.0",
        "illuminate/view": "^7.0|^8.0|^9.0",
        "nesbot/carbon": "^2.0",
        "symfony/http-kernel": "^5.0|^6.0|^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "ext-json": "*",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^8.5.8|^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Cashier\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravel\\Cashier\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        },
        "laravel": {
            "providers": [
                "Laravel\\Cashier\\CashierServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
